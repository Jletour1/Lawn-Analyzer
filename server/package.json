{
  "name": "lawn-analyzer-server",
  "version": "1.0.0",
  "description": "Backend server for Lawn Analyzer application",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "npx prisma migrate deploy",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:reset": "npx prisma migrate reset",
    "generate": "npx prisma generate",
    "seed": "ts-node src/scripts/seed.ts",
    "studio": "npx prisma studio",
    "db:setup": "npx prisma generate && npx prisma migrate dev --name init && npm run seed"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "aws-sdk": "^2.1490.0",
    "redis": "^4.6.10",
    "prisma": "^5.6.0",
    "@prisma/client": "^5.6.0",
    "openai": "^4.20.1",
    "praw": "^2.0.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "stripe": "^14.5.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "@types/compression": "^1.7.5",
    "typescript": "^5.2.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}